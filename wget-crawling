nohup wget --recursive --level=inf --no-remove-listing --wait=1 --random-wait --adjust-extension --no-clobber -e robots=off --user-agent="Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/126.0.0.0 Safari/537.36" --reject=gif,jpg,jpeg,png,svg,bmp,webp,ico,tiff,tif,mp4,mpeg,mpg,avi,mov,wmv,m4a,mp3,wav,flac,pdf,doc,docx,xls,xlsx,ppt,pptx,zip,rar,7z,tar,gz,iso,apk,msi,json,js,xml --tries=10 --waitretry=5 --exclude-directories=admin,tags,search,abuse,feed,report --no-check-certificate --continue --domains=$1 https://$1 >> $1.out 2>&1 &
